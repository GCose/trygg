{
  "name": "trygg-admin-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:cache": "eslint . --cache",
    "lint:staged": "lint-staged",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "prettier --write",
    "type-check": "tsc --noEmit --skipLibCheck",
    "type-check:watch": "tsc --noEmit --skipLibCheck --watch",
    "check-all": "yarn type-check && yarn lint && yarn format:check",
    "fix-all": "yarn type-check && yarn lint:fix && yarn format",
    "clean:next": "rm -rf .next",
    "clean:out": "rm -rf out",
    "clean:build": "rm -rf .next out build dist",
    "clean:yarn-lock": "rm -rf yarn.lock",
    "clean:node-modules": "rm -rf node_modules",
    "clean:cache": "rm -rf .next/cache node_modules/.cache",
    "clean:all": "rm -rf node_modules yarn.lock .next out build dist",
    "clean:next:cache": "next build --no-lint && rm -rf .next/cache",
    "clean:eslint-cache": "rm -rf .eslintcache",
    "clean:typescript-cache": "rm -rf tsconfig.tsbuildinfo",
    "clean:full-cache": "yarn cache clean && rm -rf .next node_modules/.cache .eslintcache tsconfig.tsbuildinfo",
    "reset": "rm -rf node_modules yarn.lock && yarn install",
    "reset:hard": "yarn clean:all && yarn install",
    "fresh-start": "yarn clean:all && yarn cache clean && yarn install"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "cookie": "^1.0.2",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.1.0",
    "sweetalert2": "^11.22.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.32.0",
    "@next/eslint-plugin-next": "^15.4.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.3.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "typescript": "^5"
  }
}
